<% port_quantity.to_i.times do %>
  <% case port_type
       when PortType.find_by_name('RJ') %>

    <i class="portRJ label-primary"></i>

  <% when PortType.find_by_name('FC') %>

    <i class="portFC label-success"></i>

  <% else %>

    <i class="portSCSI"><%= port_type.try(:name) %></i>

  <% end %>

  <%= '<br>'.html_safe unless alignment == 'horizontal' %>
<% end %>
