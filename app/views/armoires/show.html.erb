<div class="page-header">
  <%= link_to armoires_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_armoire_path(@armoire), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show armoire</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @armoire.title %></dd>

  <dt>Description:</dt>
  <dd><%= @armoire.description %></dd>

  <dt>Published:</dt>
  <dd><%= @armoire.published %></dd>

</dl>
