<div class="page-header">
  <%= link_to actes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_acte_path(@acte), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show acte</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @acte.title %></dd>

  <dt>Description:</dt>
  <dd><%= @acte.description %></dd>

  <dt>Published:</dt>
  <dd><%= @acte.published %></dd>

</dl>
