<div class="page-header">
  <%= link_to cards_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_card_path(@card), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show card</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nom :</dt>
  <dd><%= @card.name %></dd>

  <dt>Type de port :</dt>
  <dd><%= @card.port_type.try(:name) %></dd>

  <dt>Nombre de ports :</dt>
  <dd><%= @card.port_quantity %></dd>

</dl>
