<div class="page-header">
  <%= link_to modeles_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_modele_path(@modele), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show modele</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @modele.title %></dd>

  <dt>Description:</dt>
  <dd><%= @modele.description %></dd>

  <dt>Published:</dt>
  <dd><%= @modele.published %></dd>

</dl>
