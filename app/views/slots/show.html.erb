<div class="page-header">
  <%= link_to slots_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_slot_path(@slot), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show slot</h1>
</div>

<dl class="dl-horizontal">
  <dt>Position:</dt>
  <dd><%= @slot.position %></dd>

  <dt>Server:</dt>
  <dd><%= @slot.server_id %></dd>

  <dt>Valeur:</dt>
  <dd><%= @slot.valeur %></dd>

</dl>
